<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
  <title>{% if title %}{{ title }} | Xmartlabs{% else %}{{ metadata.title }}{% endif %}</title>
  {# <meta name="description" content="{% if page.excerpt %}{{ page.excerpt | strip_html | strip_newlines | truncate: 160 }}{% else %}{{ metadata.description }}{% endif %}"> #}
  {# <meta name="description" content="{{ page.excerpt | strip_html | strip_newlines | truncate: 160 if page.excerpt else metadata.description }}"> #}
  {# TODO: Review this #}
  <meta name="description" content="{{ excerpt if excerpt else metadata.description }}">
  {% if page.show %}
  {% else %}
    {% if page.dir != "/" %}
    <meta name="robots" content="noindex">
    {% endif %}
  {% endif %}
  <link rel="stylesheet" href="{{ metadata.baseurl }}/css/main.css?id=123456789">
  <link rel="stylesheet" href="{{ metadata.baseurl }}/css/xmartlabs-new-home.webflow.be16b59f1.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fluidbox/1.4.3/css/fluidbox.css">
  <link rel="canonical" href="{{ metadata.baseurl }}{{ metadata.url_blog }}{{ url | replace('index.html','') }}">
  <link rel="alternate" type="application/rss+xml" title="{{ metadata.title }}" href="{{ metadata.baseurl }}{{ metadata.url_blog }}/feed.xml" />
  <meta name="Copyright" content="&copy; 2020 Xmartlabs" />
  <link rel="shortcut icon" href="{{ metadata.baseurl }}/images/favicon.ico"/>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/fluidbox/1.4.3/jquery.fluidbox.min.js"></script>

  <script src="/js/webflow.f19375dc7.js"></script>
  <script src="/js/main.js"></script>
</head>
